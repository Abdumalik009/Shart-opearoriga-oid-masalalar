import java.util.Scanner;

public class If1 {
    public static void main(String[] args) {

        Scanner scanner = new Scanner(System.in);

        System.out.print("Sonni kiriting: ");
        int A = scanner.nextInt();




        if ( A>0) {
            A+=1;
            System.out.println(A);
        } else {
            System.out.println(A);
        }
    }
}
